.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 100rpx;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
}
.tabbar_box.iphoneX-height{
    padding-bottom: 66rpx;
}
.middle-wrapper{
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: rgb(121, 27, 27);
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}
.middle-wrapper.iphoneX-height{
  bottom: 66rpx;
}
.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
    padding:5px 0px 5px 0px;
    position: relative;
}
.tabbar_icon{
    width: 40rpx;
    height: 40rpx;
    padding-bottom: 5rpx;
}
.special-wrapper{
  position: absolute;
  /* left: 77rpx; */
  top: -46rpx;
  width: 96rpx;
  height: 96rpx;
  border-radius: 70%;
  border-top: 2rpx solid #f2f2f3;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  padding: 6rpx;
  z-index: -10;
}
.special-wrapper .tabbar_icon{
    width: 85rpx;
    height: 85rpx;
}
.special-text-wrapper{
  width: 56rpx;
  height: 56rpx;
}

text{
  padding-bottom: 20rpx;
}